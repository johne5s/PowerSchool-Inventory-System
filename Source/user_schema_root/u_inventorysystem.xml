        <field name="tagpos_Asset_Location" type="Integer"/>
        <field name="VIEW_ASSET_BARCODE" type="Boolean" defaultValue="1"/>
        <field name="VIEW_ASSET_SERIAL" type="Boolean" defaultValue="1"/>
